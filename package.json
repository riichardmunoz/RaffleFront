{
  "name": "raffle",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local --open",
    "startqa": "ng serve --configuration=qa --aot",
    "startprod": "ng serve --configuration=production --aot",
    "build": "ng build",
    "buildqa": "ng build --configuration=qa --aot",
    "buildprod": "ng build --configuration=production --aot",
    "build:dockerDev": "ng build --output-path=dist",
    "build:dockerQa": "ng build --c=qa --build-optimizer --aot --output-path=dist",
    "build:dockerProd": "ng build --c=production --build-optimizer --aot --optimization --output-path=dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stryker": "stryker run --ignoreStatic"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.6",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.6",
    "@angular/compiler": "^14.2.6",
    "@angular/core": "^14.2.6",
    "@angular/forms": "^14.2.6",
    "@angular/localize": "^14.2.6",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.6",
    "@angular/platform-browser-dynamic": "^14.2.6",
    "@angular/router": "^14.2.6",
    "@auth0/angular-jwt": "^5.0.2",
    "@azure/msal-angular": "^2.3.0",
    "@azure/msal-browser": "^2.24.0",
    "@ng-bootstrap/ng-bootstrap": "^13.1.1",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.3",
    "hammerjs": "^2.0.8",
    "ng2-pdf-viewer": "^9.1.4",
    "ngx-bar-rating": "^3.0.0",
    "rxjs": "~7.5.0",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.3.0",
    "typescript-string-operations": "^1.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-eslint/builder": "14.1.1",
    "@angular-eslint/eslint-plugin": "14.1.1",
    "@angular-eslint/eslint-plugin-template": "14.1.1",
    "@angular-eslint/schematics": "14.1.1",
    "@angular-eslint/template-parser": "14.1.1",
    "@angular/cli": "^14.2.6",
    "@angular/compiler-cli": "^14.2.6",
    "@angular/language-service": "^14.2.6",
    "@stryker-mutator/core": "^6.1.2",
    "@stryker-mutator/karma-runner": "^6.1.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "codelyzer": "^6.0.2",
    "eslint": "^8.23.0",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sonarqube-execution-reporter": "^0.1.1",
    "protractor": "^7.0.0",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.2"
  }
}
